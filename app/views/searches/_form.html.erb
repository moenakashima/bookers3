<% if user_signed_in? %>
  <%= form_with url: search_path, method: :get do |f| %>
  <%= f.text_field :content %>
  <%= f.select :model, options_for_select({"User" => "user", "Book" => "book"}) %>
  <%= f.select :method, options_for_select({"完全一致" => "perfect_match", "前方一致" => "forward_match","後方一致" => "backward_match","部分一致" => "partical_match"}) %>
  <%= f.submit '検索' %>
  <% end %>
<% end %>